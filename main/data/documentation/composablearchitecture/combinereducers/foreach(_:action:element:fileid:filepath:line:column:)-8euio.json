ext"},{"preciseIdentifier":"s:SH","text":"Hashable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"ID","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"text":"Sendable","preciseIdentifier":"s:s8SendableP","kind":"typeIdentifier"},{"kind":"text","text":", "},{"text":"Element","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","kind":"typeIdentifier","text":"Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP"},{"kind":"text","text":"\n"}]}]},{"parameters":[{"name":"toElementsState","content":[{"inlineContent":[{"text":"A writable key path from parent state to an ","type":"text"},{"code":"IdentifiedArray","type":"codeVoice"},{"type":"text","text":" of child"},{"text":" ","type":"text"},{"text":"state.","type":"text"}],"type":"paragraph"}]},{"name":"toElementAction","content":[{"inlineContent":[{"text":"A case path from parent action to an ","type":"text"},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/IdentifiedAction","isActive":true,"type":"reference"},{"type":"text","text":" of child"},{"type":"text","text":" "},{"type":"text","text":"actions."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"A reducer that will be invoked with child actions against elements of child","type":"text"},{"type":"text","text":" "},{"type":"text","text":"state."}],"type":"paragraph"}],"name":"element"},{"name":"fileID","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The fileID."}]}]},{"content":[{"inlineContent":[{"text":"The filePath.","type":"text"}],"type":"paragraph"}],"name":"filePath"},{"name":"line","content":[{"inlineContent":[{"text":"The line.","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"The column."}],"type":"paragraph"}],"name":"column"}],"kind":"parameters"},{"kind":"content","content":[{"level":2,"type":"heading","anchor":"return-value","text":"Return Value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}]}]},{"kind":"content","content":[{"text":"Discussion","anchor":"discussion","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"text":"For example, if a parent feature holds onto an array of child states, then it can perform","type":"text"},{"type":"text","text":" "},{"type":"text","text":"its core logic "},{"type":"emphasis","inlineContent":[{"type":"text","text":"and"}]},{"text":" the child’s logic by using the ","type":"text"},{"code":"forEach","type":"codeVoice"},{"type":"text","text":" operator:"}]},{"syntax":"swift","code":["@Reducer","struct Parent {","  struct State {","    var rows: IdentifiedArrayOf<Row.State>","    \/\/ ...","  }","  enum Action {","    case rows(IdentifiedActionOf<Row>)","    \/\/ ...","  }","","  var body: some Reducer<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .forEach(\\.rows, action: \\.rows) {","      Row()","    }","  }","}"],"type":"codeListing"},{"style":"tip","name":"Tip","content":[{"inlineContent":[{"type":"text","text":"We are using "},{"type":"codeVoice","code":"IdentifiedArray"},{"text":" from our","type":"text"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections"},{"text":" library because it provides a safe","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"and ergonomic API for accessing elements from a stable ID rather than positional indices."}],"type":"paragraph"}],"type":"aside"},{"type":"paragraph","inlineContent":[{"text":"The ","type":"text"},{"code":"forEach","type":"codeVoice"},{"type":"text","text":" forces a specific order of operations for the child and parent features. It"},{"type":"text","text":" "},{"type":"text","text":"runs the child first, and then the parent. If the order was reversed, then it would be"},{"type":"text","text":" "},{"text":"possible for the parent feature to remove the child state from the array, in which case the","type":"text"},{"text":" ","type":"text"},{"text":"child feature would not be able to react to that action. That can cause subtle bugs.","type":"text"}]},{"inlineContent":[{"text":"It is still possible for a parent feature higher up in the application to remove the child","type":"text"},{"text":" ","type":"text"},{"text":"state from the array before the child has a chance to react to the action. In such cases a","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"runtime warning is shown in Xcode to let you know that there’s a potential problem."}],"type":"paragraph"}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"hierarchy":{"paths":[["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/CombineReducers","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/CombineReducers\/Reducer-Implementations"]]},"sections":[],"metadata":{"title":"forEach(_:action:element:fileID:filePath:line:column:)","roleHeading":"Instance Method","externalID":"s:22ComposableArchitecture7ReducerPAAE7forEach_6action7element6fileID0H4Path4line6columnQrs011WritableKeyJ0Cy5StateQz21IdentifiedCollections0P5ArrayVyqd_1_qd__GG_s0nJ0Cy9CasePaths0S0Vy6ActionQzGAXyAA0pU0Oyqd_1_qd_0_GGGqd_2_yXEs12StaticStringVA6_S2utAMQyd_2_Rsd__AYQyd_2_Rsd_0_SHRd_1_s8SendableRd_1_AaBRd_2_r2_lF::SYNTHESIZED::s:22ComposableArchitecture15CombineReducersV","extendedModule":"ComposableArchitecture","modules":[{"name":"ComposableArchitecture"}],"fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"text":"forEach","kind":"identifier"},{"kind":"text","text":"<"},{"text":"ElementState","kind":"genericParameter"},{"text":", ","kin